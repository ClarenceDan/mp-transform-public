{
   "name": "小程序排版小助手",
   "version": "2.2",
   "description": "微信公众号文章一键轻松排版小程序链接。",
   "manifest_version": 2,
   "homepage_url": "https://github.com/ZhuPeng/mp-transform-public",
   "update_url": "http://upext.chrome.360.cn/intf.php?method=ExtUpdate.query",
   "icons": { 
       "16": "img/link-16.png",
       "32": "img/link-32.png",
       "64": "img/link-64.png",
       "128": "img/link-128.png"
   },  
   "permissions": ["tabs", "activeTab", "<all_urls>"],
   "browser_action": {},
   "background": {
       "scripts": ["background.js"],
       "persistent": false
   }
}
